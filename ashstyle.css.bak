body {
    background-color: #fffce0;
}
h1, h2, h3, h4 {
	font-family: "Helvetica", "Arial",  sans-serif;
	color: #000000;
}

p, td, li {
	font-family: "Times New Roman", serif;
	color: #000000;
}

li.menu {
	font-family: "Helvetica", "Arial",  sans-serif;
	color: #000000;
}

td.topbox {
   background-color: #202020;
}

/* Menu colors */
/* First combo was the following, but seemed a little dark
 * 4b802a = 75,128,42 (background)
 * 53b123 = 83,177,35  (hover)
 * Try
 * 50892e = 80,137,46 (not a very noticeable change)
 * 559230 = 85,146,48
 * Left the hover color alone - the lower R and B values makes it more saturated than the background
 */
td.menubox {
   background-color: #559230;
}

#menu {
	MARGIN: 0px 0px 15px; COLOR: #202020; BACKGROUND-COLOR: transparent;
}
#menu UL {
    PADDING-RIGHT: 0px; PADDING-LEFT: 0px; PADDING-BOTTOM: 0px; MARGIN: 0px; PADDING-TOP: 0px
}
#menu LI {
    LIST-STYLE-TYPE: none 
}
#menu LI A {
	PADDING-RIGHT: 0px; DISPLAY: block; PADDING-LEFT: 0px; PADDING-BOTTOM: 5px; WIDTH: 100%; COLOR: #202020; PADDING-TOP: 5px; BORDER-BOTTOM: 1px solid #202020; BACKGROUND-COLOR: transparent; TEXT-DECORATION: none
}
#menu LI A:hover {
	COLOR: #110011; BACKGROUND-COLOR: #53b123
}	

IMG.lf {
	 FLOAT: left; vertical-align: top; MARGIN-RIGHT: 10px; MARGIN-TOP: 5px;
}

IMG.rt {
	 FLOAT: right; vertical-align: top; MARGIN-LEFT: 10px; MARGIN-TOP: 5px;
}

.table_grid {
  border-collapse: collapse;
	border-style: solid;
	border-width: 1px 1px 1px 1px;
	background-color: transparent; 
	border-color: black; 
}
.td_grid_dark {
  border-collapse: collapse;
	border-style: solid;
	border-width: 1px 1px 1px 1px;
	background-color: #53b123; 
	border-color: black; 
}
.td_grid {
  border-collapse: collapse;
	border-style: solid;
	border-width: 1px 1px 1px 1px;
	background-color: transparent; 
	border-color: black; 
}

